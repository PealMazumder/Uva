#include<bits/stdc++.h>
#define pb(x) push_back(x)
#define ll long long
#define nl "\n"
using namespace std;
int main()
{
    string s,t;
    while(cin>>s>>t)
    {
    	int j = 0;
    	for(int i = 0;i<t.length(); i++)
    	{
    		if(s[j]==t[i])
    		{
    			j++;
			}
		}
		if(j>s.length()-1)
			cout<<"Yes"<<nl;
		else
			cout<<"No"<<nl;
	}
    return 0;
}
